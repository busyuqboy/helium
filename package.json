{
  "name": "flatbed",
  "version": "0.0.0",
  "description": "Web App Starter Kit",
  "author": "Towbook (https://towbook.com)",
  "repository": "github:towbook/flatbed",
  "license": "ISC",
  "scripts": {
    "dev": "ts-node-dev --project src/server/tsconfig.json src/server/index.ts",
    "build": "rimraf dist && concurrently npm:build:*",
    "build:client": "vite build",
    "build:server": "cross-env NODE_ENV=production rollup -c",
    "start": "node dist/server",
    "validate": "concurrently npm:validate:*",
    "validate:types:client": "tsc --project src/client --noEmit",
    "validate:types:server": "tsc --project src/server --noEmit",
    "validate:lint": "eslint --ignore-path .gitignore --fix .",
    "validate:format": "prettier --ignore-path .gitignore --write ."
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@stitches/react": "^0.1.9",
    "@types/compression": "^1.7.0",
    "@types/connect-history-api-fallback": "^1.3.4",
    "@types/connect-slashes": "0.0.32",
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "history": "^5.0.0",
    "modern-normalize": "^1.1.0",
    "prettier": "2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-esbuild": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "vite": "^2.3.2"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "connect-slashes": "^1.4.0",
    "dotenv": "^9.0.2",
    "express": "^4.17.1"
  }
}
